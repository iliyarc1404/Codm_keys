-- ================= ØªØ§Ø¨Ø¹ startmenu =================
function startmenu()
    local p = io.popen('tasklist /fi "IMAGENAME eq '..emu..'.exe" /fi "MEMUSAGE gt 300000" /nh', 'r')
    local procInfo = p:read('*a')
    p:close()
    local pid = tonumber(procInfo:match('%d+'))

    if not pid then
        messageDialog("You Must Start The CODM First!", mtWarning, mbOK)
        closeCE()
    else
        openProcess(pid)

        MyForm = createForm(true)
        MyForm.Caption = 'CODM Max Hack'
        MyForm.Width = 400
        MyForm.Height = 400

        -- Ø¯Ú©Ù…Ù‡â€ŒÙ‡Ø§ Ø¨Ù‡ ØµÙˆØ±Øª Ø´Ø¨Ú©Ù‡ Û²Ã—Û³ Ù…Ø±ØªØ¨ Ø´Ø¯Ù†Ø¯
        local buttons = {
            {10,10,180,60,nr,'â™¦ï¸ Head Shot'},
            {200,10,180,60,AK47,'â™¦ï¸ Unlimited Bullets'},
            {10,80,180,60,M21,'â™¦ï¸ Max Speed Running'},
            {200,80,180,60,KN,'â™¦ï¸ Max Super Jump'},
            {10,150,180,60,M4,'â™¦ï¸ Super Slide Max'},
            {200,150,180,60,MW11,'â™¦ï¸ Max Fire Rate'},
        }

        for i=1,#buttons do
            local b = createButton(MyForm)
            b.Left = buttons[i][1]
            b.Top = buttons[i][2]
            b.Width = buttons[i][3]
            b.Height = buttons[i][4]
            b.onClick = buttons[i][5]
            b.Caption = buttons[i][6]
        end
    end
end

