-- ================= ØªØ§Ø¨Ø¹ startmenu =================
function startmenu()
    local p = io.popen('tasklist /fi "IMAGENAME eq '..emu..'.exe" /fi "MEMUSAGE gt 300000" /nh', 'r')
    local procInfo = p:read('*a')
    p:close()
    local pid = tonumber(procInfo:match('%d+'))

    if not pid then
        messageDialog("You Must Start The CODM First!", mtWarning, mbOK)
        closeCE()
    else
        openProcess(pid)

        -- ÙØ±Ù… Ù‡Ú©
        MyForm = createForm(true)
        MyForm.Caption = 'Codm GL'
        MyForm.Width = 400
        MyForm.Height = 280

        local buttons = {
            {10,10,380,40,nr,'â™¦ï¸ Head â™¦ï¸'},
            {140,60,120,40,RPD,'â™¦ï¸ Red Line â™¦ï¸'},
            {270,110,120,40,M21,'â™¦ï¸ Speed Running â™¦ï¸'},
            {140,160,120,40,KN,'â™¦ï¸ High Jump â™¦ï¸'},
        }

        for i=1,#buttons do
            local b = createButton(MyForm)
            b.Left = buttons[i][1]
            b.Top = buttons[i][2]
            b.Width = buttons[i][3]
            b.Height = buttons[i][4]
            b.onClick = buttons[i][5]
            b.Caption = buttons[i][6]
        end

        local info1l = createLabel(MyForm)
        info1l.Caption= ' '
        info1l.Left = 90
        info1l.Top = 210

        local info3l = createLabel(MyForm)
        info3l.Caption= ''
        info3l.Left = 55
        info3l.Top = 250
    end
end

